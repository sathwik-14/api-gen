{
  "name": "quikjs",
  "version": "2.0.0",
  "description": "A a powerful tool designed to streamline the process of building APIs for Express.js applications",
  "main": "index.js",
  "bin": {
    "quik-g-p": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "generate": "node generate.js",
    "build": "rollup --config",
    "test": "jest --config jest.config.js",
    "coverage": "jest --config jest.config.js --coverage",
    "serve-coverage": "jest --config jest.config.js --coverage && node coverage.js",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "eslint . --fix --max-warnings=0",
    "format": "prettier . --write"
  },
  "author": "waglesathwik",
  "license": "MIT",
  "dependencies": {
    "esprima": "^4.0.1",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.16",
    "prettier": "^3.2.5",
    "recast": "^0.23.7"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.23.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "chalk": "^5.3.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "globals": "^15.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "node-notifier": "^10.0.1",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2"
  }
